<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="447.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.ubb.scs.ir.map.socialnetworkgit.UserController">
    <children>
        <VBox layoutX="72.0" layoutY="14.0">
            <children>
                <TableView fx:id="tableView" prefHeight="393.0" prefWidth="400.0">
                    <columns>
                        <TableColumn fx:id="tableColumnNume" maxWidth="100.0" minWidth="0.0" prefWidth="70.0" text="Nume" />
                        <TableColumn fx:id="tableColumnPrenume" maxWidth="100.0" prefWidth="70.0" text="Prenume" />
                        <TableColumn fx:id="actionColumn" maxWidth="300" prefWidth="200" text="Actions" />
                    </columns>
                </TableView>
            </children>
        </VBox>
        <Button fx:id="addButton" layoutX="472.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleAddUtilizator" prefHeight="50.0" prefWidth="51.0" />
        <Button fx:id="nextButton" layoutX="446.0" layoutY="407.0" mnemonicParsing="false" onAction="#handleNextPage" prefHeight="25.0" prefWidth="25.0" />
        <Button fx:id="previousButton" layoutX="72.0" layoutY="407.0" mnemonicParsing="false" onAction="#handlePreviousPage" prefHeight="25.0" prefWidth="25.0" />
        <TextField fx:id="textField" layoutX="14.0" layoutY="14.0" onAction="#handleTextChange" prefHeight="25.0" prefWidth="51.0" />
        <Label layoutX="14.0" layoutY="39.0" prefHeight="50.0" prefWidth="51.0" text="Utilizatori/      pg." wrapText="true">
            <font>
                <Font size="11.0" />
            </font>
        </Label>
        <Label fx:id="pagesLabel" layoutX="272.0" layoutY="411.0" text="Label" />
        <TextField fx:id="pagesTextField" layoutX="246.0" layoutY="407.0" onAction="#selectPage" prefHeight="25.0" prefWidth="25.0" />
    </children>
</AnchorPane>
